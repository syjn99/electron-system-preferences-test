{
  "name": "electron-system-preferences-test",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "license": "MIT",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
